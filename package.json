{
  "name": "ts-doubly-linked-list",
  "version": "1.0.1",
  "description": "Doubly Linked List",
  "main": "dist/linked-list.js",
  "types": "dist/linked-list.d.ts",
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "jest": "26.6.3",
    "ts-jest": "26.5.0",
    "typescript": "4.9.5"
  },
  "scripts": {
    "test": "jest --changedSince=master",
    "build": "eslint --cache 'src/**/*.{ts,tsx}' && tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix"
  },
  "jest": {
    "rootDir": "./",
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "transform": {
      "^.+\\.ts?$": "<rootDir>/node_modules/ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testEnvironment": "node"
  },
  "keywords": ["linked-list"],
  "author": "",
  "license": "ISC"
}
